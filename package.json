{
  "name": "boundary-sdk",
  "version": "2.0.1",
  "description": "API Normalization SDK - A TypeScript-first SDK that enforces a single stable contract across all third-party API providers",
  "main": "dist/public.js",
  "types": "dist/public.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/public.d.ts",
      "import": "./dist/public.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "biome check .",
    "format": "biome format --write .",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "api",
    "sdk",
    "normalization",
    "circuit-breaker",
    "rate-limiting",
    "retry"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Raghaverma/Boundary"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^20.14.0",
    "typescript": "^5.5.0",
    "vitest": "^1.6.0"
  }
}
